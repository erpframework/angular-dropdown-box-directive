{
  "name": "AngularDropdownBox",
  "description": "Dropdown Box - AngularJS Directive",
  "version": "1.0.0",
  "homepage": "",
  "author": "Jonas Antonell",
  "private": false,
  "assets": {
    "config": {
      "name": "dropdown-box",
      "less": "assets/less",
